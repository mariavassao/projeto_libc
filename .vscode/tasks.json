{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Build com libft",
			"command": "C:\\MinGW\\bin\\gcc.exe",
			"args": [
				"-Wall",
				"-Wextra",
				"-g",
				"${fileDirname}\\main.c",
				"${fileDirname}\\ft_isalpha.c",
				"-o",
				"${fileDirname}\\main.exe"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": ["$gcc"],
			"group": "build"
		}
	]
}
